{   "[typescript]": {
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    },
    "eslint.validate": [
        "typescript",
      ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "todo-tree.filtering.excludeGlobs": [
        "docsDependencies",
        "node_modules",
        "build",
        "__test__",
        ".git",
        "docs",
        "start.js"
    ]
}